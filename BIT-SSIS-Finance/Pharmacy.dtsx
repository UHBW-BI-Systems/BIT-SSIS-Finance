                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select	
 -- CONVERT(char, convert(datetime,[issue_date],103),112) as stored_date_of_issue,
  puhb.issue_date as issue_date,
--  drugfull as approved_name,
  puhb.drugfull as drugfull,
  --CAST(null as nvarchar(30)) as Dose_Form_Text,
  --CAST(null as nvarchar(25)) as Strength,
 -- costcentre as ward_or_department,
