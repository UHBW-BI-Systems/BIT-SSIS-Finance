  puhb.PbR_Exclusion as PbR_Exclusion,
  puhb.BNSSG as BNSSG,
  puhb.MPS as MPS,
  puhb.Uveitis as Uveitis,
  puhb.Homecare as Homecare, 
  puhb.Hepatology as Hepatology,
  puhb.Blueteq as Blueteq,
 
  puhb.location_code,
  --puhb.location_code as location_code,
  --left([location_code], 3) as Location_Code,
  --drug_packsize as Units_Of_Issue_Text,
  puhb.drug_packsize as drug_packsize,   
  
  --CAST(null as varchar(10)) as issue_point_details,
  --left(CAST( [costcentre_lcode] AS VARCHAR(15)),15) AS Cost_Centre,	
  puhb.costcentre_lcode,
  --puhb.costcentre_lcode as costcentre_lcode,
	
  --left(CAST(rtrim([patient_hospitalno]) as varchar(15)),15) as patient_identifier,
  puhb.patient_hospitalno as patient_hospitalno,

  --left(convert(char, convert(datetime,[issue_date],103),112),6) as period_code, 

  puhb.bnf_code as bnf_code,
  puhb.DataSource as DataSource,

--  puhb.czid AS return_id,
  puhb.czid,
  puhb.lnkdid,

  puhb.c_drugname,
  puhb.c_strength,
  puhb.c_form,

  --puhb.issue_date AS stored_date_of_issue_dt,

  --puhb.Unit_Price AS supplier_unit_price,
  puhb.supplier_unit_price,  
  puhb.costcentre_fcode,
--puhb.Drug_Code AS dm_d,
  puhb.DmdId,

  puhb.quantity_containers,
--  puhb.quantity_containers AS quantity_containers,
puhb.quantity_doseunits,
--puhb.quantity_doseunits AS quantity_doseunits,
  puhb.QuantityNo,
--  puhb.QuantityNo AS QuantityNo,
  puhb.unit_description,
  puhb.ReturnQuantity,
  puhb.DmdName,
  puhb.DMDType,

  puhb.c_national_stock_no,

  puhb.data_source_id,
  pds.description as data_source_description,

  puhb.[DRUG NAME],
  puhb.[DRUG STRENGTH],
  puhb.[DRUG VOLUME],
  puhb.[DRUG PACK],

  puhb.QuantityDecimal,
  puhb.UoM,
  puhb.DrugPackSize,
  puhb.ReturnDecimal,
  puhb.QuantityNett

-- 0 as old_report_flag

FROM Finance.import.tbl_pharmacy_UHB AS puhb
  join dbo.tbl_pharmacy_data_source  AS pds ON pds.id = puhb.data_source_id</property>
