                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT xdi.patient_hospitalno,
  CAST(FORMAT(xdi.issue_date,'yyyyMMdd') AS NUMERIC) AS issue_date,
  s.local_patient_identifier,
  s.gp_code_original_data,
  s.gp_practice_code_original_data,
  s.pct_derived_from_gp_practice --ph.pct_of_residence
FROM [Finance].[import].[pharmacy_WGH] xdi WITH (NOLOCK)
JOIN SUS.dbo.sus_apc_spell_pbr_current s
ON s.local_patient_identifier = xdi.patient_hospitalno
WHERE xdi.issue_date BETWEEN DATEADD("Month", DATEDIFF("Month", -1, GETDATE()) - 3, 0) AND (GETDATE())
  AND xdi.issue_typeid IN (1,2,3,4,5,6,7,8,20)
ORDER BY	issue_date, xdi.patient_hospitalno</property>
