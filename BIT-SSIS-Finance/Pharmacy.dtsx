                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT distinct xdi.patient_hospitalno,
  xdi.issue_date,
  ph.patient_id,
  ph.regd_gp_code, 
  ph.regd_practice_code, 
  ph.patient_first_name, 
  ph.patient_last_name,
  ph.valid_from_date,
  ph.valid_to_date,
  ph.pct_of_residence
FROM JAC.dbo.mds_xdaily_issues_BOO xdi WITH (NOLOCK)
  join PHD.dbo.tbl_patient_history ph
--		  ON ph.patient_id = ('T' + xdi.patient_hospitalno)
  ON ph.patient_trust_number = ('T' + xdi.patient_hospitalno)
--WHERE		xdi.issue_date BETWEEN ? AND ?
WHERE xdi.issue_date BETWEEN DATEADD("Month", DATEDIFF("Month", -1, getdate()) - 2, 0) AND (GETDATE())
  AND xdi.issue_date BETWEEN  ph.valid_from_date AND ph.valid_to_date
  AND xdi.issue_typeid IN (1,2,3,4,5,6,7,8,20)
ORDER BY	xdi.patient_hospitalno, xdi.issue_date, ph.valid_from_date</property>
