  --CAST(ExVATValue as decimal(15,2)) as Change_In_Stock_Value_ex_vat,
  puhb.ExVATValue as ExVATValue,
  --CAST(issue_value as decimal(15,2)) as Change_In_Stock_Value, 
  puhb.issue_value as issue_value,
  --left(convert(nvarchar(10), [issue_typeid]),10) as issue_type_code,
  puhb.issue_typeid as issue_typeid,

  puhb.Chemo as Chemo,
  puhb.NICE as NICE,
