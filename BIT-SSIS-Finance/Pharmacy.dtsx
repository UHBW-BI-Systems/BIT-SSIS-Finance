            <component
              refId="Package\Data Flow from JAC database\Source for JAC Boots"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Source for JAC Boots"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Queries to avoid duplicates as multiple pack sizes

WITH		VMPP AS
(
SELECT		DISTINCT
			vmp.VPID,
			--vmp.NM,
			vmp.QTVAL,
			vmp.QTY_UOMCD,
			uom.[DESC]
FROM		JAC.dmd.vmpp$ vmp WITH (NOLOCK)
INNER JOIN	JAC.dmd.UoM$ uom WITH (NOLOCK)
			ON vmp.QTY_UOMCD = uom.CD
--where		vmp.VPID = '14614511000001102'
),

VMPP2 AS
(
SELECT		DISTINCT
			vmp.VPID,
			--vmp.NM,
			--vmp.QTVAL,
			vmp.QTY_UOMCD,
			uom.[DESC]
FROM		JAC.dmd.vmpp$ vmp WITH (NOLOCK)
INNER JOIN	JAC.dmd.UoM$ uom WITH (NOLOCK)
			ON vmp.QTY_UOMCD = uom.CD
)

SELECT		xdi.issue_date, --AS 'Date',
			xdi.drugfull, --AS 'Drug',
			xdi.drug_packsize, --AS 'Pack',
			xdi.costcentre, -- AS 'Cost Centre',
			xdi.Quantity,
			CAST(xdi.issue_value AS decimal(10,2)) AS 'issue_value', -- AS 'Value',
			CAST(CASE WHEN db.c_vatcode = 'Z' THEN xdi.issue_value
					 WHEN db.c_vatcode = 'F' THEN xdi.issue_value/1.05
					 WHEN db.c_vatcode = 'S' THEN xdi.issue_value/1.2
					 END
				 AS decimal(10,2)) AS 'ExVATValue',
			xdi.issue_typeid, --AS 'Issue Type',
			xdi.location_code, -- AS 'Location Code',
			xdi.costcentre_lcode, -- AS 'Ledger Code',
			--xdi.costcentre_code AS 'Cost Centre Code',
			xdi.bnf_code, -- AS 'BNF',
			xdi.patient_hospitalno, -- AS 'Patient Hospital Number',
			xdi.patient_natno, -- AS 'NHS Number',
			CASE WHEN xdi.bnf_code LIKE '08.01%' OR xdi.bnf_code LIKE '08.02.03%' OR xdi.bnf_code LIKE '08.02.04%' THEN 'Chemotherapy Drug' ELSE NULL END AS 'Chemo',
			CASE WHEN NICE.speccon_description IS NULL THEN NULL ELSE 'NICE' END AS 'NICE',
			CASE WHEN NICEOH.speccon_description IS NULL THEN NULL ELSE 'NICEOH' END AS 'NICE Chemo',
			CASE WHEN PbR.speccon_description IS NULL THEN NULL ELSE 'PBREX' END AS 'PbR Exclusion',
			CASE WHEN BNSSG.speccon_description IS NULL THEN NULL ELSE 'BNSSG' END AS 'BNSSG',
			CASE WHEN MPS.speccon_description IS NULL THEN NULL ELSE 'MPS' END AS 'MPS',
			CASE WHEN Uve.speccon_description IS NULL THEN NULL ELSE 'UVEITIS' END AS 'Uveitis',
			CASE WHEN HC.speccon_description IS NULL THEN NULL ELSE 'Homecare' END AS 'Homecare',
			CASE WHEN HP.speccon_description IS NULL THEN NULL ELSE 'Hepatology' END AS 'Hepatology',
			CASE WHEN BT.speccon_description IS NULL THEN NULL ELSE 'Blueteq' END AS 'Blueteq',
			db.c_drugname,
			db.c_strength,
			db.c_form,
			dmd.DmdId, -- AS 'Drug Code',
			CASE WHEN dmd.DmdTypeId = 1 THEN 'VMP'
				 WHEN dmd.DmdTypeId = 3 THEN 'AMP'
				 ELSE NULL
				 END AS 'DMDType',
			CASE WHEN xdi.issue_value = 0 OR ISNULL(xdi.quantity_containers,xdi.quantity_doseunits) = 0 THEN 0
					  ELSE xdi.issue_value / CAST(CASE WHEN xdi.quantity_containers IS NULL THEN xdi.quantity_doseunits
			  		  ELSE xdi.quantity_containers * (CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END)
				      END / CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END AS decimal(10,4)) END AS 'supplier_unit_price' ,-- AS 'Unit Price',
			--xdi.supplier_unit_price AS 'Unit Price',
			xdi.costcentre_fcode,
			xdi.czid,
--			xdi.quantity_containers,
			xdi.quantity_doseunits,
			CASE WHEN xdi.quantity_containers IS NULL THEN xdi.quantity_doseunits
				 ELSE xdi.quantity_containers * (CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END)
				 END AS 'QuantityNo',
			dp.unit_description,
			(xdi.container_returns * CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END) + xdi.doseunit_returns AS 'ReturnQuantity',
			dmd.DmdName,
			xdi.quantity_containers,
			xdi.lnkdid,
			dbc.c_national_stock_no,
			'1' AS data_source_id, 
			'JAC' AS datasource,
			--CASE WHEN xdi.username LIKE '%1' THEN 'TRUE' ELSE NULL END AS 'TRUE',
			--mds.*
			mds.[DRUG NAME],
			mds.[DRUG STRENGTH],
			mds.[DRUG VOLUME],
			mds.[DRUG PACK],
			CAST(CASE WHEN xdi.quantity_containers IS NULL THEN xdi.quantity_doseunits
			  		  ELSE xdi.quantity_containers * (CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END)
				      END / CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END AS decimal(10,4)) AS 'QuantityDecimal',								-- Fraction of full container
			ISNULL(ISNULL(vmp.UDFS_UOMCD,VMPP.QTY_UOMCD),VMPP2.QTY_UOMCD) AS 'UoM',																					-- Unit of Measure from dm+d, join depends on item
			CASE WHEN dp.pack_magnitude = 0 THEN 1 
				 --WHEN dp.du_cont &lt;&gt; dp.pack_magnitude THEN dp.du_cont
				 WHEN dp.unit_description IN ('mL','g','l','mg','kg') THEN 1
				 ELSE dp.pack_magnitude END AS 'DrugPackSize',																										-- Packsize for items
			CAST(((xdi.container_returns * CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END) + xdi.doseunit_returns)
				/
				CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END as decimal(10,4)) AS 'ReturnDecimal',
			CAST(CASE WHEN xdi.quantity_containers IS NULL THEN xdi.quantity_doseunits
			  		  ELSE xdi.quantity_containers * (CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END)
				      END / CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END AS decimal(10,4))
				-
				CAST(((xdi.container_returns * CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END) + xdi.doseunit_returns)
				/
				CASE WHEN dp.pack_magnitude = 0 THEN 1 ELSE dp.pack_magnitude END as decimal(10,4)) AS 'QuantityNett'
FROM		JAC.dbo.mds_xdaily_issues_BOO xdi WITH (NOLOCK)
LEFT JOIN	JAC.dbo.drug_basic_common dbc WITH (NOLOCK)
			ON xdi.lnkdid = dbc.lnkdid
--LEFT JOIN	JAC.dbo.disp_labp_details LAB with (nolock)
--			on RIGHT(XDI.comment,8) = LAB.batch
LEFT JOIN	JAC.dbo.MDS_LOOKUP MDS WITH (NOLOCK)
			ON xdi.lnkdid = mds.DrugId
			AND xdi.lnkdpz = mds.lnkdpz
LEFT JOIN	JAC.dbo.drug_speccon_BOO NICE WITH (NOLOCK)
			ON xdi.lnkdid = NICE.lnkdid
			AND NICE.speccon_description = 'NICE Drugs (Excluding Oncology and Haematology)'
LEFT JOIN	JAC.dbo.drug_speccon_BOO NICEOH WITH (NOLOCK)
			ON xdi.lnkdid = NICEOH.lnkdid
			AND NICEOH.speccon_description = 'NICE Drugs for Oncology and Haematology'
LEFT JOIN	JAC.dbo.drug_speccon_BOO PbR WITH (NOLOCK)
			ON xdi.lnkdid = PbR.lnkdid
			AND PbR.speccon_description = 'PbR drug exclusions'
LEFT JOIN	JAC.dbo.drug_speccon_BOO BNSSG WITH (NOLOCK)
			ON xdi.lnkdid = BNSSG.lnkdid
			AND BNSSG.speccon_description = 'BNSSG Commissioning report'
LEFT JOIN	JAC.dbo.drug_speccon_BOO MPS WITH (NOLOCK)
			ON xdi.lnkdid = MPS.lnkdid
			AND MPS.speccon_description = 'MPS drug reporting'
LEFT JOIN	JAC.dbo.drug_speccon_BOO Uve WITH (NOLOCK)
			ON xdi.lnkdid = Uve.lnkdid
			AND Uve.speccon_description = 'Uveitis contract'
LEFT JOIN	JAC.dbo.drug_speccon_BOO HC WITH (NOLOCK)
			ON xdi.lnkdid = HC.lnkdid
			AND HC.speccon_description = 'Homecare Services'
LEFT JOIN	JAC.dbo.drug_speccon_BOO HP WITH (NOLOCK)
			ON xdi.lnkdid = HP.lnkdid
			AND HP.speccon_description = 'Hepatology'
LEFT JOIN	JAC.dbo.drug_speccon_BOO BT WITH (NOLOCK)
			ON xdi.lnkdid = BT.lnkdid
			AND BT.speccon_description = 'Blueteq'
LEFT JOIN	JAC.dbo.drug_pack_BOO dp WITH (NOLOCK)
			ON xdi.lnkdid = dp.lnkdid AND xdi.lnkdpz = dp.lnkdpz
LEFT JOIN	JAC.dbo.drug_basic_BOO db WITH (NOLOCK)
			ON xdi.lnkdid = db.lnkdid
LEFT JOIN	JAC.dbo.Drug dmd WITH (NOLOCK)
			ON xdi.lnkdid = dmd.DrugId
LEFT JOIN	JAC.dmd.ampType$ amp WITH (NOLOCK)
			ON dmd.DmdId = amp.apid
LEFT JOIN	vmpp WITH (NOLOCK)
			ON ISNULL(amp.vpid,dmd.DmdId) = vmpp.vpid
			AND CASE WHEN xdi.lnkdid = 'D19698' THEN 177		-- Anomaly where pack quantity is 4 x 177
					 WHEN xdi.lnkdid = 'D13822' THEN 75			-- Anomaly where pack quantity is 3 x 25
					ELSE dp.pack_magnitude END = VMPP.QTVAL	
LEFT JOIN	JAC.dmd.vmp$ vmp WITH (NOLOCK)
			ON ISNULL(amp.vpid,dmd.DmdId) = vmp.vpid
LEFT JOIN	vmpp2 WITH (NOLOCK)
			ON ISNULL(amp.vpid,dmd.DmdId) = vmpp2.vpid
			AND VMPP.VPID IS NULL						-- Only to use if cannot match quantity of pack

WHERE		xdi.issue_date BETWEEN DATEADD("Month", DATEDIFF("Month", -1, getdate()) - 2, 0) AND (GETDATE())

--WHERE		DATEPART(MONTH,xdi.issue_date) = DATEPART(MONTH,DATEADD(MONTH,-1,GETDATE()))-- BETWEEN '2022-01-07' AND '2022-31-07'
--			AND	DATEPART(YEAR,xdi.issue_date) = DATEPART(YEAR,DATEADD(MONTH,-1,GETDATE()))
			AND xdi.issue_typeid IN (1,2,3,4,5,6,7,8,20)
			AND	CASE WHEN dp.unit_description LIKE '%ml' AND ISNULL(VMPP.[DESC],VMPP2.[DESC]) IN ('gram','litre') THEN 1
				 WHEN dp.unit_description LIKE '%g' AND ISNULL(VMPP.[DESC],VMPP2.[DESC]) LIKE '%ml' THEN 1
				 ELSE 0 END = 0
			--AND dmd.DmdId IS NULL
			--AND xdi.issue_value &gt; 0
ORDER BY	xdi.issue_date, drugfull</property>
