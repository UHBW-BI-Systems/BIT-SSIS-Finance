                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT xdi.Patient_Identifier,
	xdi.stored_date_of_issue_dt,
	s.start_date_hospital_provider_spell,
    s.end_date_hospital_provider_spell,
	s.hospital_provider_spell_no,
	s.treatment_function_code

FROM Finance.transform.pharmacy xdi WITH (NOLOCK)
  JOIN SUS.dbo.sus_apc_spell_pbr_current s
  ON s.local_patient_identifier = xdi.Patient_Identifier
WHERE xdi.stored_date_of_issue_dt BETWEEN DATEADD("Month", DATEDIFF("Month", -1, getdate()) - 3, 0) AND (GETDATE())
  AND xdi.stored_date_of_issue_dt BETWEEN s.start_date_hospital_provider_spell AND s.end_date_hospital_provider_spell --m.spell_start_date AND m.spell_end_date
  AND xdi.Issue_Type_Code IN (1,2,3,4,5,6,7,8,20)
AND s.treatment_function_code !='999'
ORDER BY	xdi.Patient_Identifier, xdi.stored_date_of_issue_dt</property>
